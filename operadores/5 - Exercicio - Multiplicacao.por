programa {
  funcao inicio() {
    //Multiplicação: Faça um programa que leia dois números e exiba a multiplicação deles.

    real numero1 
    real numero2

    escreva("Digite o primeiro número: ")
    leia(numero1)

    escreva("\nDigite o segundo número: ")
    leia(numero2)

    real multiplicacao = numero1 * numero2

    escreva("\nA multiplicação dos dois números digitados é ", multiplicacao)
  }
}
