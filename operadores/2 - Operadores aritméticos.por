programa {
  funcao inicio() {

  //Adição
  real soma = 4.5 + 7
  escreva(soma, "\n")

  //Substração
  real substracao = 9.1 - 3.4
  escreva(substracao, "\n")

  //Multiplicação 
  inteiro multiplicacao = 5 * 5
  escreva(multiplicacao, "\n")

  //Divisão
  real divisao = 5 / 2
  escreva(divisao, "\n")

  //Módulo
  inteiro modulo = 5 % 2
  escreva(modulo)

  }
}
