programa {
  funcao inicio() {
    
    //Números
    inteiro idade = 29
    escreva(idade, "\n")
    
    real preco = 2.50
    escreva(preco, "\n")

    //Texto
    caracter vogal = 'A'
    cadeia nome = "Fernando"

    escreva(vogal, "\n")
    escreva(nome, "\n")

    //Lógico
    logico dia = verdadeiro
    logico noite = falso

    escreva(dia, "\n")
    escreva(noite)
  }
}
